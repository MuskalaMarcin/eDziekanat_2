    <!-- Security constraints -->
    <security-constraint>
        <display-name>HomeSecurityConstraint</display-name>
        <web-resource-collection>
            <web-resource-name>HomePage</web-resource-name>
            <url-pattern>/home/*</url-pattern>
            <url-pattern>/classrooms/*</url-pattern>
